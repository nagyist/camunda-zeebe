    "eslint-plugin-license-header": "0.9.0",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.5.0",
    "eslint-plugin-testing-library": "7.16.0",
    "globals": "17.3.0",
    "jsdom": "26.1.0",
    "knip": "5.84.1",
    "monaco-editor": "0.55.1",
    "msw": "2.12.10",
