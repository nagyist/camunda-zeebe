[
  {
    "extension": "revapi.filter",
    "id": "filter",
    "configuration": {
      "archives": {
        "justification": "Ignore everything not included in the module itself",
        "include": [
          "io\\.camunda:camunda-spring-boot-starter:.*"
        ]
      },
      "elements": {
        "exclude": [
          {
            "justification": "The implementation package is not meant to be used directly, and as such does not need to maintain any backwards compatibility guarantees.",
            "matcher": "java-package",
            "match": "/io\\.camunda\\.client\\.impl(\\..*)?/"
          },
          {
            "justification": "Annotation processor internals are not part of the public API surface.",
            "matcher": "java-package",
            "match": "/io\\.camunda\\.client\\.spring\\.annotation\\.processor(\\..*)?/"
          },
          {
            "justification": "Test support classes are not part of the public API surface.",
            "matcher": "java-package",
            "match": "/io\\.camunda\\.client\\.spring\\.testsupport(\\..*)?/"
          },
          {
            "justification": "Spring events are not part of the public API surface and should be excluded.",
            "matcher": "java-package",
            "match": "/io\\.camunda\\.client\\.spring\\.event(\\..*)?/"
          },
          {
            "justification": "Actuator classes are not part of the public API surface and should be excluded.",
            "matcher": "java-package",
            "match": "/io\\.camunda\\.client\\.spring\\.actuator(\\..*)?/"
          },
          {
            "justification": "JobHandling classes are not part of the public API surface and should be excluded.",
            "matcher": "java-package",
            "match": "/io\\.camunda\\.client\\.spring\\.jobhandling(\\..*)?/"
          },
          {
            "justification": "Annotation value classes are not part of the public API surface and should be excluded.",
            "matcher": "java-package",
            "match": "/io\\.camunda\\.client\\.annotation\\.value(\\..*)?/"
          }
        ]
      }
    }
  },
